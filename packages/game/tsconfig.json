{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "models/*": [
        "models/*"
      ],
      "utils/*": [
        "utils/*"
      ],
      "constants/*": [
        "constants/*"
      ],
      "store/*": [
        "store/*"
      ],
      "app/*": [
        "app/*"
      ],
      "components/*": [
        "app/components"
      ]
    }
  },
  "include": [
    "src",
    "src/Modules/**.ts"
  ]
}
